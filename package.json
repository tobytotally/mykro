{
  "name": "mykro-prototype",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 9000",
    "build": "tsc && vite build",
    "preview": "vite preview --host 0.0.0.0 --port 9000",
    "start": "vite preview --host 0.0.0.0 --port 9000",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "screenshot": "npx tsx scripts/screenshot-pages.ts",
    "screenshot:mobile": "npx tsx scripts/screenshot-pages.ts --mobile",
    "screenshot:all": "npx tsx scripts/screenshot-pages.ts --all",
    "screenshot:betting-states": "npx tsx scripts/betting-slip-states.ts",
    "screenshot:betting-states:mobile": "npx tsx scripts/betting-slip-states.ts --mobile",
    "screenshot:betting-states:all": "npx tsx scripts/betting-slip-states.ts --all",
    "deploy:gh-pages": "npm run build && gh-pages -d dist",
    "deploy:build": "npm run build && npm run preview"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-image-crop": "^11.0.10",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.10.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.3.0",
    "playwright": "^1.55.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.20.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
